#include<iostream>
#include<vector>
using namespace std;
#define QUEUE 8

void queue();
void lay(int i);

vector<int> a(1+QUEUE,0),b(1+3*QUEUE,0),c(1+3*QUEUE,0);
int ans=0;

void queue(){

    lay(1);

    cout<<ans<<endl;
}

void lay(int i){
    int j=0;

    while(++j <= QUEUE){

        if(a[j]+b[QUEUE+j-i]+c[QUEUE+j+i]==0){

            a[j]=b[QUEUE+j-i]=c[QUEUE+j+i]=1;

            if(i<QUEUE) 
                lay(i+1);
            else
                ++ans;
        
            a[j]=b[QUEUE+j-i]=c[QUEUE+j+i]=0;  // back here
        }

    }
    
}


int main(){
    queue();
    
    return 0;
}
